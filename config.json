{
	"font": "Monospace Regular 14",
	"tags": [ "term", "chat", "files", "text", "5", "music", "7", "vm", "9", "@", "v" ],
	"terminal": "urxvt",
	"dockposition": "left",
	"picomfreezeworkaround": 0,
	"keys": [
		 { "key": "Return",    "modifiers": [ "mod4", "shift" ],   "action": { "function": "spawnterm", "arg": 0 } },
		 { "key": "Return",    "modifiers": [ "mod4" ],            "action": { "function": "zoom", "arg": 0 } },
		 { "key": "t",         "modifiers": [ "mod4" ],            "action": { "function": "setlayout", "arg": "tile" } },
		 { "key": "f",         "modifiers": [ "mod4" ],            "action": { "function": "setlayout", "arg": "float" } },
		 { "key": "m",         "modifiers": [ "mod4" ],            "action": { "function": "setlayout", "arg": "monocle" } },
		 { "key": "s",         "modifiers": [ "mod4" ],            "action": { "function": "setlayout", "arg": "spiral" } },
		 { "key": "p",         "modifiers": [ "mod4" ],            "action": { "function": "spawn", "arg": "albert show" } },
		 { "key": "Tab",       "modifiers": [ "mod1" ],            "action": { "function": "spawn", "arg": "rofi -modi window -show window -hide-scrollbar -padding 50 -line-padding 4 -auto-select -kb-cancel \"Alt+Escape,Escape\" -kb-accept-entry \"!Alt-Tab,!Alt+Alt_L,Return,Alt_L,Alt_R\" -kb-row-down \"Alt+Tab,Alt+Down\" -kb-row-up \"Alt+ISO_Left_Tab,Alt+Up\"" } },
		 { "key": "b",         "modifiers": [ "mod4" ],            "action": { "function": "togglebar" } },
		 { "key": "j",         "modifiers": [ "mod4" ],            "action": { "function": "focusstack", "arg": 1 } },
		 { "key": "k",         "modifiers": [ "mod4" ],            "action": { "function": "focusstack", "arg": -1 } },
		 { "key": "h",         "modifiers": [ "mod4" ],            "action": { "function": "setmfact", "arg": -0.05 } },
		 { "key": "l",         "modifiers": [ "mod4" ],            "action": { "function": "setmfact", "arg": 0.05 } },
		 { "key": "Tab",       "modifiers": [ "mod4" ],            "action": { "function": "view" } },
		 { "key": "c",         "modifiers": [ "mod4", "shift" ],   "action": { "function": "killclient" } },
		 { "key": "d",         "modifiers": [ "mod4" ],            "action": { "function": "toggledock" } },
		 { "key": "z",         "modifiers": [ "mod4" ],            "action": { "function": "togglefoldtags" } },
		 { "key": "BackSpace", "modifiers": [ "mod4" ],            "action": { "function": "rotatemonitor" } },
		 { "key": "space",     "modifiers": [ "mod4", "shift" ],   "action": { "function": "togglefloating" } },
		 { "key": "comma",     "modifiers": [ "mod4" ],            "action": { "function": "focusmon", "arg": -1 } },
		 { "key": "period",    "modifiers": [ "mod4" ],            "action": { "function": "focusmon", "arg": 1 } },
		 { "key": "comma",     "modifiers": [ "mod4", "shift" ],   "action": { "function": "tagmon", "arg": -1 } },
		 { "key": "period",    "modifiers": [ "mod4", "shift" ],   "action": { "function": "tagmon", "arg": 1 } },
		 { "key": "j",         "modifiers": [ "mod4", "control" ], "action": { "function": "pushdown" } },
		 { "key": "k",         "modifiers": [ "mod4", "control" ], "action": { "function": "pushup" } },
		 { "key": "q",         "modifiers": [ "mod4", "shift" ],   "action": { "function": "quit" } },
		 { "key": "t",         "modifiers": [ "mod4", "control" ], "action": { "function": "rotatelayoutaxis", "arg": 0 } },
		 { "key": "m",         "modifiers": [ "mod4", "control" ], "action": { "function": "rotatelayoutaxis", "arg": 1 } },
		 { "key": "s",         "modifiers": [ "mod4", "control" ], "action": { "function": "rotatelayoutaxis", "arg": 2 } },
		 { "key": "Return",    "modifiers": [ "mod4", "control" ], "action": { "function": "mirrorlayout" } },
		 { "key": "l",         "modifiers": [ "mod4", "control" ], "action": { "function": "shiftmastersplit", "arg": -1 } },
		 { "key": "h",         "modifiers": [ "mod4", "control" ], "action": { "function": "shiftmastersplit", "arg": 1 } },
		 { "key": "u",         "modifiers": [ "mod4" ],            "action": { "function": "toggleswallow" } },
		 { "key": "1",         "modifiers": [ "mod4" ],            "action": { "function": "tagkeys", "arg": "term" } },
		 { "key": "2",         "modifiers": [ "mod4" ],            "action": { "function": "tagkeys", "arg": "chat" } },
		 { "key": "3",         "modifiers": [ "mod4" ],            "action": { "function": "tagkeys", "arg": "files" } },
		 { "key": "4",         "modifiers": [ "mod4" ],            "action": { "function": "tagkeys", "arg": "text" } },
		 { "key": "5",         "modifiers": [ "mod4" ],            "action": { "function": "tagkeys", "arg": "5" } },
		 { "key": "6",         "modifiers": [ "mod4" ],            "action": { "function": "tagkeys", "arg": "music" } },
		 { "key": "7",         "modifiers": [ "mod4" ],            "action": { "function": "tagkeys", "arg": "7" } },
		 { "key": "8",         "modifiers": [ "mod4" ],            "action": { "function": "tagkeys", "arg": "vm" } },
		 { "key": "9",         "modifiers": [ "mod4" ],            "action": { "function": "tagkeys", "arg": "9" } },
		 { "key": "0",         "modifiers": [ "mod4" ],            "action": { "function": "tagkeys", "arg": "@" } },
		 { "key": "minus",     "modifiers": [ "mod4" ],            "action": { "function": "tagkeys", "arg": "v" } }
	],
	"buttons": [
		{ "click": "ltsymbol", "button": 1, "action": { "function": "toggledock" } },
		{ "click": "ltsymbol", "button": 2, "action": { "function": "setlayout", "arg": "tile" } },
		{ "click": "ltsymbol", "button": 3, "action": { "function": "setlayout", "arg": "monocle" } },
		{ "click": "ltsymbol", "button": 4, "action": { "function": "viewscroll", "arg": -0.05 } },
		{ "click": "ltsymbol", "button": 5, "action": { "function": "viewscroll", "arg": 0.05 } },
		{ "click": "wintitle", "button": 1, "action": { "function": "zoom" } },
		{ "click": "wintitle", "button": 2, "action": { "function": "killclient" } },
		{ "click": "wintitle", "button": 3, "action": { "function": "tagmon", "arg": 1 } },
		{ "click": "wintitle", "button": 4, "action": { "function": "opacitychange", "arg": -0.02 } },
		{ "click": "wintitle", "button": 5, "action": { "function": "opacitychange", "arg": 0.02 } },
		{ "click": "wintitle", "button": 6, "action": { "function": "shiftmastersplit", "arg": 1 } },
		{ "click": "wintitle", "button": 7, "action": { "function": "shiftmastersplit", "arg": -1 } },
		{ "click": "wintitle", "button": 8, "action": { "function": "focusstack", "arg": -1 } },
		{ "click": "wintitle", "button": 9, "action": { "function": "focusstack", "arg": 1 } },
		{ "click": "statustext", "button": 1, "action": { "function": "spawn", "arg": "urxvt -name ghost_terminal -e /home/tinou/hacks/scripts/network_status.sh" } },
		{ "click": "statustext", "button": 2, "action": { "function": "spawn", "arg": "qdbus org.kde.kglobalaccel /component/kmix invokeShortcut \"mute\"" } },
		{ "click": "statustext", "button": 3, "action": { "function": "rotatemonitor", "arg": 1 } },
		{ "click": "statustext", "button": 4, "action": { "function": "spawn", "arg": "qdbus org.kde.kglobalaccel /component/kmix invokeShortcut \"increase_volume\"" } },
		{ "click": "statustext", "button": 5, "action": { "function": "spawn", "arg": "qdbus org.kde.kglobalaccel /component/kmix invokeShortcut \"decrease_volume\"" } },
		{ "click": "statustext", "button": 9, "action": { "function": "sendselkey", "arg": "Left" } },
		{ "click": "statustext", "button": 8, "action": { "function": "sendselkey", "arg": "Right" } },
		{ "click": "clientwin", "button": 1, "modifiers": [ "mod4" ], "action": { "function": "movemouse" } },
		{ "click": "clientwin", "button": 2, "modifiers": [ "mod4" ], "action": { "function": "togglefloating" } },
		{ "click": "clientwin", "button": 3, "modifiers": [ "mod4" ], "action": { "function": "resizemouse" } },
		{ "click": "tagbar", "button": 1, "action": { "function": "view" } },
		{ "click": "tagbar", "button": 3, "action": { "function": "toggleview" } },
		{ "click": "tagbar", "button": 4, "action": { "function": "increasebright" } },
		{ "click": "tagbar", "button": 5, "action": { "function": "decreasebright" } },
		{ "click": "tagbar", "button": 9, "action": { "function": "togglefoldtags" } },
		{ "click": "tagbar", "button": 8, "action": { "function": "tabview", "arg": -1 } },
		{ "click": "tagbar", "button": 1, "modifiers": [ "mod4" ], "action": { "function": "tag" } },
		{ "click": "tagbar", "button": 3, "modifiers": [ "mod4" ], "action": { "function": "toggletag" } }
